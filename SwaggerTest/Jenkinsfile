pipeline{
    agent any
    stages{
        stage('Build'){
            steps {
                 git 'https://github.com/PhongNguyen520/Jenkins.git'
            }
        }
        stage('PushDocker'){
            steps {
                withDockerRegistry(credentialsId: 'docker-hub', url: 'https://hub.docker.com/') {
                    sh 'docker build -t aspnetcoretest .'
                    sh 'docker push aspnetcoretest'
                }
            }
        }

    }
}